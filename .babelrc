{
    "presets": ["react", "es2015"],
    "env": {
        "development": {
            "plugins": [["react-transform", {
                "transforms": [{
                    "transform": "react-transform-hmr",
                    // If you use React Native, pass "react-native" instead:
                    "imports": ["react"],
                    // this is important for webpack HMR:
                    "locals": ["module"]
                }]
            }]]
        }
    }
}
